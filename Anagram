    bool isAnagram(string a, string b){
        
        // Your code here
        int n=a.size();
        int m=b.size();
    int k=n;
    int l=m;
    if(k !=l)
    return false;
        sort(a.begin(),a.end());
        sort(b.begin(),b.end());
    
    for(int i=0;i<k;i++)
    {
        if(a[i] !=b[i])
        return false;
         
    }
   return true;
    }
