int searchInsertK(vector<int>Arr, int N, int k)
    {
        // code here
        for(int i=0;i<N;i++)
        {
            if(Arr[i]==k)
            {
                return i;
            }
            else if(Arr[i]>k)
            {
                return i;
            }
        }
        return N;
    }
