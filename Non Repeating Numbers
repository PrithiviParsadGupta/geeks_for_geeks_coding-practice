class Solution
{
public:
    vector<int> singleNumber(vector<int> nums) 
    {
        // Code here.
        vector<int>ans;
        int n=nums.size();
        map<int,int>mp;
        for(int i=0;i<n;i++)
        {
            mp[nums[i]]++;
        }
      
        for(auto val:mp)
        {
            
            if(val.second==1)
            {
               
           ans.push_back(val.first);
          
        }
        }
             
        return ans;
    }
};
